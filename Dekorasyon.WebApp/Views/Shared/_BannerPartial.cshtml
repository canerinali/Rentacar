@using Blog.Entities
@using Dekorasyon.WebApp.Models
@{
    List<Home> list = CacheHelper.GetHomesFromCache();
}
@foreach (Home home in list)
{
    <div id="home" class="parallax first-section" data-stellar-background-ratio="0.4" style="background-image:url('/images/@home.HomeImage');">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12 col-md-offset-2 text-center">
                    <div class="big-tagline">
                        <img class="border-line-img" src="~/constructed/uploads/building/sep-line-white.png" alt="" />
                        <h2><span class="yellow">@home.Title</span></h2>
                        <img style="margin-bottom:40px;" class="border-line-img" src="~/constructed/uploads/building/sep-line-white.png" alt="" />

                        <a data-scroll href="#projects" class="btn btn-light btn-radius btn-brd">@home.Description</a>
                    </div>
                </div>
            </div>
            <!-- end row -->
        </div>
        <!-- end container -->
    </div>
}